{
  "manifest_version": 3,
  "name": "computer simulator",
  "description": "The computer simulator.",
  "version": "1.0.0",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "storage",
    "alarms"
  ],
  "action": {
    "default_popup": "options.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}